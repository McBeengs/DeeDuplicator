{
  "name": "deeduplicator",
  "version": "0.0.1",
  "author": "",
  "description": "An electron-vue project",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "build": {
    "productName": "deeduplicator",
    "appId": "com.mcbeengs.deeduplicator",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*",
      "src/workers/**/*",
      "src/services/**/*",
      "src/db/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "amd-loader": "0.0.8",
    "async": "^3.1.0",
    "better-sqlite3": "^5.4.3",
    "bootstrap-vue": "^2.2.0",
    "electron-window-manager": "^1.0.6",
    "ffmpeg-extract-frame": "^1.0.2",
    "ffmpeg-generate-video-preview": "^1.0.3",
    "ffprobe": "^1.1.0",
    "hamming": "0.0.2",
    "imghash": "0.0.6",
    "jimp": "^0.9.3",
    "js-levenshtein": "^1.1.6",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "regenerator-runtime": "^0.13.3",
    "string-similarity": "^4.0.1",
    "sweet-modal-vue": "^2.0.0",
    "trash": "^6.1.1",
    "vue": "^2.5.16",
    "vue-easycm": "^1.0.4",
    "vue-electron": "^1.0.6",
    "vue-lazyload": "^1.3.3",
    "vue-multiselect-listbox": "0.0.20",
    "vue-progress": "^0.2.1",
    "vue-router": "^3.0.1",
    "vue-sidebar-menu": "^4.4.7",
    "vue-step-progress": "^0.3.1",
    "vuebar": "0.0.20",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0",
    "workerpool": "^5.0.4"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.4.8",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "4.0.3",
    "electron-builder": "^21.2.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.7.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "resolutions": {
    "clean-css": "^4.1.11",
    "lodash.merge": "^4.6.2",
    "yargs-parser": "^18.1.2"
  }
}
